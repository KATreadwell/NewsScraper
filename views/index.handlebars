<div class="jumbotron text-center">
  <div class="overlay"></div>
  <div class="background-image"></div>
  <div class="caption">
    <h1>Mongo Scraper</h1>
    <h2>NY Times Opinion: Technology</h2>
    <h3>All the Articles fit to be scraped!</h3>
  </div>
</div>

<div class="container-fluid article-container">
  <div class="card">
    <div class="card-header">
      <ul>
        {{#each Articles}}
        <li>{{title}}
          <br>
          {{summary}}
          <br>
          {{link}}</li>
        <a class="btn btn-success save" id="saveArticle" data-id="{{_id}}">Save Article</a>
        {{/each}}
      </ul>
    </div>
  </div>
</div>


 
